<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="/img/logo_tra.png" sizes="32x32">
    <title>@ViewBag.Title TimeWaltz</title>
    <partial name="_SylesheetPartial" />
    @await RenderSectionAsync("Sylesheets", false)
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <partial name="_NavbarPartial" />
        <partial name="_MainSidebarPartial" />

        <div class="content-wrapper">
            @RenderBody()
            
        </div>

        <partial name="_FooterPartial" />

        <aside class="control-sidebar control-sidebar-dark">

        </aside>
    </div>
    
    <script src="/plugins/jquery/jquery.min.js"></script>
    
    <script src="/plugins/jquery-ui/jquery-ui.min.js"></script>
    
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    
    <script src="/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <partial name="_PluginsScriptsPartial" />    
    <script src="/js/adminlte.js"></script>    
    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
    @await RenderSectionAsync("Scripts", false)
</body>
</html>